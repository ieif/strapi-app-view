{
  "name": "vite-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "buildxxxx": "vue-tsc && vite build",
    "build": "cross-env NODE_ENV=production vite build",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-npm.js",
    "preview": "vite preview",
    "format": "run-s format:*",
    "format:ts": " prettier --write  \"src/**/*.ts\" "
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@vue/apollo-option": "^4.0.0-alpha.20",
    "core-js": "^3.6.4",
    "dayjs": "^1.11.7",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "koa": "^2.14.1",
    "vue": "^3.2.45",
    "vue-markdown-it": "^0.9.4",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  }
}
