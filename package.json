{
  "name": "vite-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "dev": "cross-env NODE_ENV=development node server.js",
    "dev-vite": "vite",
    "buildxxxx": "vue-tsc && vite build",
    "buildxxx": "cross-env NODE_ENV=production vite build",
    "build": "run-s build:server build:client",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/server.js",
    "preview": "vite preview",
    "format": "run-s format:*",
    "format:ts": " prettier --write  \"src/**/*.ts\" "
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@koa/router": "^12.0.0",
    "@vue/apollo-option": "^4.0.0-alpha.20",
    "core-js": "^3.6.4",
    "dayjs": "^1.11.7",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "koa": "^2.14.1",
    "koa-compress": "^5.1.0",
    "koa-connect": "^2.1.0",
    "koa-static": "^5.0.0",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-markdown-it": "^0.9.4",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  }
}
